# Reddit API Credentials
# Obtain these from: https://www.reddit.com/prefs/apps
# Create a "script" type application

REDDIT_CLIENT_ID=your_client_id_here
REDDIT_CLIENT_SECRET=your_client_secret_here
REDDIT_USERNAME=your_reddit_username
REDDIT_PASSWORD=your_reddit_password

# YouTube API Credentials
# Obtain these from: https://console.cloud.google.com/
# Enable YouTube Data API v3 and create OAuth 2.0 credentials

YOUTUBE_CLIENT_ID=your_youtube_client_id
YOUTUBE_CLIENT_SECRET=your_youtube_client_secret

# X/Twitter API Credentials
# Obtain these from: https://developer.x.com/en/portal/dashboard
# Create an app with OAuth 2.0 (User authentication with PKCE)

X_CLIENT_ID=your_client_id_here
X_CLIENT_SECRET=your_client_secret_here

# LinkedIn API Credentials
# Obtain these from: https://www.linkedin.com/developers/apps
# Create an app with "Share on LinkedIn" and "Sign In with LinkedIn using OpenID Connect"

LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret

# Cloudinary API Credentials
# Obtain these from: https://console.cloudinary.com/
# Find in Dashboard > Programmable Media > API Keys

CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
